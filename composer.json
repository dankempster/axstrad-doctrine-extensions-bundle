{
    "name": "axstrad/doctrine-extensions-bundle",
    "description": "Symfony 2 Bundle for Axstrad\\DoctrineExtensions",
    "type": "symfony-bundle",
    "keywords": ["doctrine", "extensions", "symfony bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "~5.4"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Bundle\\DoctrineExtensionsBundle\\": ""
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
