{
    "name": "axstrad/doctrine-extensions-bundle",
    "description": "Symfony 2 Bundle for Axstrad\\DoctrineExtensions",
    "type": "symfony-bundle",
    "keywords": ["doctrine", "extensions", "symfony bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.4",
        "axstrad/common": ">=0.1",
        "axstrad/doctrine-extensions": ">=0.1",
        "doctrine/doctrine-bundle": "~1",
        "doctrine/orm": "~2.3",
        "symfony/symfony": ">=2.3 <2.7"
    },

    "require-dev": {
        "axstrad/test": "~0.1.0",
        "axstrad/test-bundle": "~0.1.0",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "phpunit/phpunit": "~4.1@stable",
        "sensio/framework-extra-bundle": "~2.3 || ~3.0"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Bundle\\DoctrineExtensionsBundle\\": ""
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
